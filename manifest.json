{
  "manifest_version": 2,

  "name": "groupLister",
  "description": "Collects youtube urls and turns them into a playlist",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs", "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery-1.11.2.min.js"]
    }
  ]
}
