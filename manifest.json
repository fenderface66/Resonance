{
  "manifest_version": 2,

  "name": "groupLister",
  "description": "Collects youtube urls and turns them into a playlist",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click to collect links"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.11.2.min.js", "contentScript.js"]
    }
  ]
}
